<launch>
  <param name="/use_sim_time" value="true" />
  <node name="cn" pkg="cartographer_ros" type="cartographer_offline_node" args="
     -configuration_directory $(find xycar_slam)/config
     -configuration_basenames mapping.lua
     -urdf_filenames $(find xycar_slam)/urdf/xycar.urdf
     -bag_filenames /home/nvidia/Desktop/test.bag
     -load_state_filename $(find xycar_slam)/maps/gangnam.pbstream" />
     
  <node name="rviz" pkg="rviz" type="rviz" args=" -d $(find xycar_slam)/rviz/localization.rviz" />
  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" type="cartographer_occupancy_grid_node" args="-resolution 0.05" />

</launch>
