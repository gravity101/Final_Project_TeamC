<launch>
  <param name="robot_description" textfile="$(find xycar_slam)/urdf/xycar.urdf" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <include file="$(find xycar_motor)/launch/xycar_motor.launch" />
  <include file="$(find xycar_lidar)/launch/lidar_noviewer.launch" />
  <include file="$(find xycar_imu)/launch/xycar_imu_9dof_noviewer.launch" />
  <!-- <include file="$(find joystick_cam)/launch/joy_cam.launch" /> -->

  <node name="cartographer_node" pkg="cartographer_ros" type="cartographer_node" args="
    -configuration_directory $(find xycar_slam)/config
    -configuration_basename mapping.lua" />
  <node name="cartographer_occupancy_grid_node" pkg="cartographer_ros" type="cartographer_occupancy_grid_node"
    args="-resolution 0.05"/>

  <node name="rviz" pkg="rviz" type="rviz" args=" -d $(find xycar_slam)/carto.rviz"/>
  <!--node name="rosbag" pkg="rosbag" type="record" args="-O /home/nvidia/Desktop/final2.bag /tracked_pose /scan /imu /odom /tf /tf_static /clock"/-->
  <node name="curv" pkg="xycar_slam" type="curvature.py" />
</launch>
